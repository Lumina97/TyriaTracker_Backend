{
	"name": "guildwars2-ts",
	"version": "1.2.2",
	"description": "GuildWars 2 API Wrapper in Typescript",
	"homepage": "https://gitlab.com/dinckelman/guildwars2-ts",
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"files": ["dist/", "LICENSE", "README.md", "package.json"],
	"packageManager": "pnpm@9.14.2",
	"scripts": {
		"prepare": "npm run lint && npm run build",
		"lint:dry": "biome check .",
		"lint": "biome check . --write",
		"build": "tsup --env.NODE_ENV production",
		"test": "jest",
		"test:coverage": "jest --coverage"
	},
	"license": "ISC",
	"keywords": ["guildwars", "guildwars2", "guild wars", "guild wars 2", "gw", "gw2"],
	"dependencies": {
		"axios": "1.7.8",
		"guildwars2-ts": "file:",
		"promise-queue": "2.2.5",
		"tslog": "4.9.3",
		"zod": "3.23.8"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.6.3",
		"@jest/globals": "^29.7.0",
		"@types/node": "^20.12.2",
		"@types/promise-queue": "^2.2.3",
		"jest": "^29.7.0",
		"ts-jest": "^29.1.2",
		"ts-node": "^10.9.2",
		"tsup": "^8.0.2",
		"typescript": "^5.4.3"
	}
}
